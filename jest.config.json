{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
        "^@platform/(.*).js": [
            "<rootDir>/platform/$1"
        ],
        "^@lib/(.*).js": [
            "<rootDir>/lib/$1"
        ],
        "^@bin/(.*).js": [
            "<rootDir>/lib/bin/$1"
        ],
        "^@cli/(.*).js": [
            "<rootDir>/lib/cli/$1"
        ],
        "^@core/(.*).js": [
            "<rootDir>/lib/core/$1"
        ],
        "^@errors/(.*).js": [
            "<rootDir>/lib/errors/$1"
        ],
        "^@factory/(.*).js": [
            "<rootDir>/lib/factory/$1"
        ],
        "^@generators/(.*).js": [
            "<rootDir>/lib/generators/$1"
        ],
        "^@messages/(.*).js": [
            "<rootDir>/lib/messages/$1"
        ],
        "^@logger/(.*).js": [
            "<rootDir>/lib/logger/$1"
        ],
        "^@patches/(.*).js": [
            "<rootDir>/lib/patches/$1"
        ],
        "^@prompts/(.*).js": [
            "<rootDir>/lib/prompts/$1"
        ],
        "^@tools/(.*).js": [
            "<rootDir>/lib/tools/$1"
        ],
        "^@interfaces/(.*).js": [
            "<rootDir>/lib/interfaces/$1"
        ],
        "^@transformers/(.*).js": [
            "<rootDir>/lib/transformers/$1"
        ],
        "^@testing/(.*).js": [
            "<rootDir>/lib/testing/$1"
        ],
        "^@specs/(.*).js": [
            "<rootDir>/specs/$1"
        ],
        "^@structure/(.*).js": [
            "<rootDir>/structure/$1"
        ],
        "(.*)\\.js$": [
            "$1"
        ]
    },
    "notify": true,
    "moduleFileExtensions": [
        "js",
        "json",
        "ts",
        "template"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
        "^.+\\.m?[tj]s?$": [
            "ts-jest",
            {
                "useESM": true,
                "tsconfig": "tsconfig.jest.json"
            }
        ]
    },
    "collectCoverageFrom": [
        "**/*.(t)s"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
        "dist",
        "dist:check",
        "dist:specs",
        "coverage",
        "specs",
        "node_modules"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/specs/specs.setup.ts"
    ],
    "modulePathIgnorePatterns": [
        "example"
    ],
    "extensionsToTreatAsEsm": [
        ".ts"
    ]
}